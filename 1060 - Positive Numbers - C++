#include<bits/stdc++.h>
using namespace std;

#define optimize() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define endl '\n';

int main(){
    
    optimize()

    int x = 0;
    
    for(int i = 0; i < 6 ; i++){
        float store;
        cin >> store;
        if(store > 0){
            x= x+1;
        }
    }
    cout<<x<<" valores positivos"<<endl;
    return 0;
}
